<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.united.dailymed">

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DAILYMED">

        <activity android:name=".activity_heart_report">
        </activity>

        <activity android:name=".activity_heart_tips">
        </activity>

        <activity android:name=".activity_heart_history">

        </activity>

        <activity android:name=".activity_heart_add_entry">
        </activity>

        <activity android:name=".Fitness" />

        <activity android:name=".Pill" />

        <activity android:name=".Water" />

        <activity android:name=".Heart">
        </activity>

        <activity android:name=".Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>